{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "babel-plugin-espower",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "presets": [
        ["env", { "targets": { "node": 8 }}]
      ]
    }
  }
}
